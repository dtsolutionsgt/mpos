<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D0000000"
    tools:context=".CliPos" >

    <RelativeLayout
        android:id="@+id/relclipos"
        android:layout_width="match_parent"
        android:layout_height="410dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="false"
        android:layout_centerVertical="false"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginRight="2dp"
        android:background="#FFFFFF"
        android:paddingBottom="1dp">

        <TextView
            android:id="@+id/textView184"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textView2"
            android:layout_alignStart="@+id/textView2"
            android:layout_alignParentLeft="false"
            android:layout_marginStart="2dp"
            android:layout_marginTop="80dp"
            android:paddingBottom="3dp"
            android:text="Correo"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#000000"
            android:textSize="18sp"
            android:visibility="invisible" />

        <RelativeLayout
            android:id="@+id/relRoad"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/frame_rect_blue">

            <RelativeLayout
                android:id="@+id/relRoadbar"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_centerVertical="true"
                android:background="#9A999E"></RelativeLayout>

            <ImageView
                android:id="@+id/imgTitLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="3dp"
                android:layout_marginBottom="5dp"
                android:adjustViewBounds="true"
                android:src="@drawable/logo_panel" />

            <ImageView
                android:id="@+id/imgRoadTit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/relRoadbar"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:adjustViewBounds="true"
                android:padding="5dp"
                android:src="@drawable/cli_white" />

            <RelativeLayout
                android:id="@+id/relPed"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="5dp">

                <TextView
                    android:id="@+id/textView177"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/imageView68"
                    android:background="#FF2020"
                    android:gravity="center"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:text="00"
                    android:textColor="#FFFFFF"
                    android:textSize="30sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <TextView
                android:id="@+id/lblTit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgRoadTit"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="Cliente"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#FFFFFF" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView1"
            android:layout_alignStart="@+id/textView1"
            android:layout_marginStart="0dp"
            android:layout_marginTop="25dp"
            android:paddingBottom="3dp"
            android:text="Nombre"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#000000"
            android:textSize="18sp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView154"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textView2"
            android:layout_alignStart="@+id/textView2"
            android:layout_alignParentLeft="false"
            android:layout_marginStart="0dp"
            android:layout_marginTop="25dp"
            android:paddingBottom="3dp"
            android:text="Dirección"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#000000"
            android:textSize="18sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="568dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt1"
            android:layout_alignStart="@+id/txt1"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            android:hint="Nombre"
            android:maxLines="2"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/editText1"
            android:layout_width="1132dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText2"
            android:layout_alignStart="@+id/editText2"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="10dp"
            android:ems="10"
            android:hint="Dirección" />

        <EditText
            android:id="@+id/txtCorreo"
            android:layout_width="1132dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/editText1"
            android:layout_alignStart="@+id/editText1"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="130dp"
            android:ems="10"
            android:hint="Correo"
            android:inputType="textEmailAddress" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/textView6"
            android:layout_alignParentRight="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/imageView53"
            android:background="@drawable/frame_btn"
            android:clickable="true"
            android:gravity="center"
            android:onClick="consFinal"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="Consumidor Final"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="166dp"
            android:layout_height="42dp"
            android:layout_below="@+id/editTextNumber4"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="false"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/frame_btn"
            android:clickable="true"
            android:gravity="center"
            android:onClick="clienteNIT"
            android:text="Cliente con NIT"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView53"
            android:layout_width="100dp"
            android:layout_height="42dp"
            android:layout_below="@+id/textView6"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/frame_btn"
            android:clickable="true"
            android:onClick="buscarCliente"
            android:padding="8dp"
            android:src="@drawable/consultasblanco_gris" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="4dp"
            android:layout_below="@+id/textView4"
            android:layout_centerHorizontal="true"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relRoad"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="false"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="24dp"
            android:text="NIT"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="15sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relRoad"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="130dp"
            android:ems="10"
            android:hint="NIT"
            android:inputType="none"
            android:singleLine="true"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="40dp"
            android:layout_height="5dp"
            android:layout_below="@+id/editText2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="48dp"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/imageView65"
            android:layout_width="24dp"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/textView1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="-12dp"
            android:src="@drawable/cli_nit" />

        <ImageView
            android:id="@+id/imageView66"
            android:layout_width="24dp"
            android:layout_height="36dp"
            android:layout_alignTop="@+id/editText2"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="false"
            android:layout_marginStart="5dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="1071dp"
            android:src="@drawable/cli_name" />

        <ImageView
            android:id="@+id/imageView67"
            android:layout_width="24dp"
            android:layout_height="28dp"
            android:layout_alignTop="@+id/editText1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="6dp"
            android:src="@drawable/cli_dir" />

        <ImageView
            android:id="@+id/imageView76"
            android:layout_width="24dp"
            android:layout_height="28dp"
            android:layout_alignTop="@+id/textView5"
            android:layout_alignParentStart="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/envelope" />

        <ImageView
            android:id="@+id/imageView99"
            android:layout_width="24dp"
            android:layout_height="40dp"
            android:layout_below="@+id/imageView76"
            android:layout_alignParentStart="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/phone2" />

        <TextView
            android:id="@+id/textView223"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView184"
            android:layout_alignTop="@+id/imageView99"
            android:layout_marginStart="0dp"
            android:layout_marginTop="8dp"
            android:text="Teléfono"
            android:textColor="#000000"
            android:textSize="18sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/editTextNumber4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtCorreo"
            android:layout_alignStart="@+id/txtCorreo"
            android:layout_alignEnd="@+id/txtCorreo"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:ems="10"
            android:hint="Teléfono"
            android:inputType="number" />

        <ProgressBar
            android:id="@+id/progressBar4"
            style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Large"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true" />

        <CheckBox
            android:id="@+id/chkPickup"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/editTextNumber4"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="10dp"
            android:text="  PICKUP"
            android:textColor="#000000"
            android:textColorHighlight="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />

        <CheckBox
            android:id="@+id/checkBox21"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/relRoad"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            android:text="  LLEVAR"
            android:textColor="#000000"
            android:textColorHighlight="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView237"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/checkBox21"
            android:layout_alignBottom="@+id/checkBox21"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="-210dp"
            android:layout_marginEnd="34dp"
            android:layout_marginBottom="215dp"
            android:background="@drawable/frame_stat"
            android:clickable="true"
            android:onClick="doDomicilio"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            android:text="DOMICILIO"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView238"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/editTextNumber4"
            android:layout_below="@+id/editText1"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/frame_btn"
            android:clickable="true"
            android:gravity="center"
            android:onClick="doDireccion"
            android:paddingStart="15dp"
            android:paddingEnd="15dp"
            android:text="LISTA"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:text="LISTA" />

    </RelativeLayout>

</RelativeLayout>
