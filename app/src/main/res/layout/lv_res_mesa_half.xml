<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rellayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frame_rect"
    tools:context=".Img_view_list">

    <RelativeLayout
        android:id="@+id/relframe"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/frame_rect_3">

        <RelativeLayout
            android:id="@+id/relcolor"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="4dp"
            android:background="#D0D0D0">

            <TextView
                android:id="@+id/textView324"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblV1"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="0dp"
                android:gravity="center_horizontal"
                android:text="TextView"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView323"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/imageView130"
                android:layout_alignBottom="@+id/lblV5"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="0dp"
                android:gravity="center_horizontal"
                android:text="TextView"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold"
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/imageView130"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblV1"
                android:layout_alignBottom="@+id/lblV5"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/ic_nosync" />

            <TextView
                android:id="@+id/textView270"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:ellipsize="middle"
                android:gravity="top|left"
                android:lines="2"
                android:maxLines="2"
                android:text="TextView"
                android:textSize="40sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblV1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:text="M1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000"
                android:textSize="36sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblV5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView97"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="false"
                android:layout_marginStart="35dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="10dp"
                android:paddingBottom="10dp"
                android:text="00:00"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/imageView97"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignBottom="@+id/lblV1"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="0dp"
                android:layout_toEndOf="@+id/lblV1"
                android:adjustViewBounds="true"
                android:padding="2dp"
                android:scaleType="centerInside"
                android:src="@drawable/icon_cooking2" />


            <TextView
                android:id="@+id/textView272"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/lblV5"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="0dp"
                android:text="TextView"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/imageView148"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView272"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="2dp"
                android:adjustViewBounds="true"
                android:foregroundGravity="right"
                android:src="@drawable/table_icon" />

        </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>

