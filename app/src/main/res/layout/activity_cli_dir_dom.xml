<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CliDirDom">

        <RelativeLayout
            android:id="@+id/relclipos"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="false"
            android:layout_centerVertical="false"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="2dp"
            android:background="#DBF5F3"
            android:paddingBottom="1dp">

            <TextView
                android:id="@+id/textView184"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView154"
                android:layout_alignStart="@+id/textView154"
                android:layout_alignParentLeft="false"
                android:layout_marginStart="0dp"
                android:layout_marginTop="9dp"
                android:paddingBottom="3dp"
                android:text="Referencia"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#000000"
                android:textSize="18sp" />

            <RelativeLayout
                android:id="@+id/relRoad"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="@drawable/frame_rect_blue">

                <RelativeLayout
                    android:id="@+id/relRoadbar"
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_centerVertical="true"
                    android:background="#9A999E"></RelativeLayout>

                <ImageView
                    android:id="@+id/imgTitLogo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="3dp"
                    android:layout_marginBottom="5dp"
                    android:adjustViewBounds="true"
                    android:src="@drawable/logo_panel" />

                <ImageView
                    android:id="@+id/imgRoadTit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/relRoadbar"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:adjustViewBounds="true"
                    android:padding="5dp"
                    android:src="@drawable/cli_white" />

                <RelativeLayout
                    android:id="@+id/relPed"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="5dp"/>

                <TextView
                    android:id="@+id/lblTit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imgRoadTit"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="Direccion"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#FFFFFF" />
            </RelativeLayout>

            <TextView
                android:id="@+id/textView192"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView184"
                android:layout_marginStart="3dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/imageView80"
                android:paddingBottom="3dp"
                android:text="Telefono"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView154"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/editText1"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="false"
                android:layout_marginStart="33dp"
                android:layout_marginTop="3dp"
                android:paddingBottom="3dp"
                android:text="DirecciÃ³n"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#000000"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/editText1"
                android:layout_width="1132dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/editText2"
                android:layout_alignEnd="@+id/editText2"
                android:layout_alignRight="@id/textView4"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="50dp"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="0dp"
                android:ems="10"
                android:maxLength="250"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtCorreo"
                android:layout_width="1132dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/editText1"
                android:layout_alignEnd="@+id/editText1"
                android:layout_alignRight="@id/editText1"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="86dp"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="0dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="594dp"
                android:layout_height="67dp"
                android:layout_alignTop="@+id/textView242"
                android:layout_alignBottom="@+id/textView6"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="-1dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="-1dp"
                android:layout_toRightOf="@+id/textView5"
                android:background="@drawable/frame_btn"
                android:clickable="true"
                android:gravity="center"
                android:onClick="doSave"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:text="APLICAR"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="226dp"
                android:layout_height="66dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="180dp"
                android:layout_marginRight="108dp"
                android:layout_toLeftOf="@+id/textView5"
                android:background="@drawable/frame_btn"
                android:clickable="true"
                android:gravity="center"
                android:onClick="doExit"
                android:text="SALIR"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="4dp"
                android:layout_below="@+id/textView4"
                android:layout_centerHorizontal="true"
                android:text="Small Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="invisible" />

            <EditText
                android:id="@+id/txt15"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relRoad"
                android:layout_alignParentLeft="false"
                android:layout_marginStart="28dp"
                android:layout_marginTop="77dp"
                android:layout_marginRight="0dp"
                android:layout_toEndOf="@+id/textView192"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:singleLine="true"
                android:textSize="24sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:id="@+id/relbotpan"
                android:layout_width="wrap_content"
                android:layout_height="5dp"
                android:layout_below="@+id/TextView01"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="1dp"
                android:layout_marginBottom="1dp"
                android:background="@drawable/blue_strip"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:gravity="center_horizontal"
                    android:paddingRight="10dp"
                    android:text="DTSolutions"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp" />
            </RelativeLayout>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="40dp"
                android:layout_height="5dp"
                android:layout_below="@+id/editText2"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="48dp"
                android:text="Small Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/imageView67"
                android:layout_width="21dp"
                android:layout_height="23dp"
                android:layout_alignTop="@+id/textView154"
                android:layout_alignParentStart="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="6dp"
                android:src="@drawable/cli_dir" />

            <ImageView
                android:id="@+id/imageView76"
                android:layout_width="24dp"
                android:layout_height="22dp"
                android:layout_alignTop="@+id/textView184"
                android:layout_alignParentStart="true"
                android:layout_marginStart="4dp"
                android:layout_marginTop="4dp"
                android:src="@drawable/note" />

            <ImageView
                android:id="@+id/imageView80"
                android:layout_width="25dp"
                android:layout_height="24dp"
                android:layout_below="@+id/relRoad"
                android:layout_alignStart="@+id/textView5"
                android:layout_alignParentEnd="false"
                android:layout_marginStart="-612dp"
                android:layout_marginTop="83dp"
                android:padding="2dp"
                android:src="@drawable/phone" />

            <TextView
                android:id="@+id/textView242"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView6"
                android:layout_alignBottom="@+id/textView6"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="0dp"
                android:background="@drawable/frame_btn"
                android:gravity="center"
                android:paddingStart="20dp"
                android:paddingEnd="20dp"
                android:text="ELIMINAR"
                android:textSize="30sp"
                android:textStyle="bold" />

        </RelativeLayout>
    </RelativeLayout>