<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PedidoDet">

    <RelativeLayout
        android:id="@+id/relRoad"
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:background="@drawable/frame_rect_blue">

        <ImageView
            android:id="@+id/imageView69"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:clickable="true"
            android:onClick="doExit"
            android:padding="10dp"
            android:src="@drawable/break_w" />

        <ImageView
            android:id="@+id/imageView70"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:adjustViewBounds="true"
            android:padding="20dp"
            android:src="@drawable/menu" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/relRoad"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/frame_rect">

        <RelativeLayout
            android:id="@+id/rel03"
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_below="@id/rel02"
            android:layout_alignParentTop="false"
            android:layout_margin="5dp"
            android:background="@drawable/frame_btn">

            <ImageView
                android:id="@+id/imageView74"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_marginStart="3dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="18dp"
                android:adjustViewBounds="true"
                android:padding="4dp"
                android:src="@drawable/ped_3" />

            <TextView
                android:id="@+id/textView182"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/imageView74"
                android:text="00"
                android:textColor="#000000"
                android:textSize="52sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView183"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView182"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="2dp"
                android:gravity="center_horizontal"
                android:text="Completos"
                android:textColor="#000000"
                android:textSize="12sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel02"
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_below="@id/rel01"
            android:layout_alignParentTop="false"
            android:layout_margin="5dp"
            android:background="@drawable/frame_btn">

            <ImageView
                android:id="@+id/imageView73"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_marginStart="3dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="18dp"
                android:adjustViewBounds="true"
                android:padding="4dp"
                android:src="@drawable/ped_2" />

            <TextView
                android:id="@+id/textView181"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/imageView73"
                android:text="00"
                android:textColor="#000000"
                android:textSize="52sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView180"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView181"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="2dp"
                android:gravity="center_horizontal"
                android:text="Pendientes"
                android:textColor="#000000"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel01"
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_margin="5dp"
            android:background="@drawable/frame_btn">

            <TextView
                android:id="@+id/textView178"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView179"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="2dp"
                android:gravity="center_horizontal"
                android:text="Nuevos"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/imageView71"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="false"
                android:layout_marginStart="3dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="18dp"
                android:adjustViewBounds="true"
                android:padding="4dp"
                android:src="@drawable/ped_1" />

            <TextView
                android:id="@+id/textView179"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/imageView71"
                android:text="00"
                android:textColor="#000000"
                android:textSize="52sp"
                android:textStyle="bold" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>