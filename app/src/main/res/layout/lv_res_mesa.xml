<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rellayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frame_rect"
    tools:context=".Img_view_list">

    <RelativeLayout
        android:id="@+id/relframe"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/frame_rect_3">

        <RelativeLayout
            android:id="@+id/relcolor"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="4dp"
            android:background="#D0D0D0">

            <TextView
                android:id="@+id/lblV1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:text="M1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000"
                android:textSize="64sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblV3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblV1"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="75dp"
                android:layout_marginTop="20dp"
                android:text="2"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000"
                android:textSize="36sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblV4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblV3"
                android:layout_alignStart="@+id/lblV3"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="75dp"
                android:layout_marginTop="20dp"
                android:paddingBottom="20dp"
                android:text="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000"
                android:textSize="36sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblV5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblV3"
                android:layout_alignParentLeft="false"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:text="00:00"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="36sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/imageView97"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/lblV3"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:padding="10dp"
                android:scaleType="fitEnd"
                android:src="@drawable/icon_cooking2" />

            <ImageView
                android:id="@+id/imageView98"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblV3"
                android:layout_alignBottom="@+id/lblV3"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="0dp"
                android:adjustViewBounds="true"
                android:padding="10dp"
                android:src="@drawable/icon_users" />

            <ImageView
                android:id="@+id/imageView99"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lblV4"
                android:layout_alignBottom="@+id/lblV4"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="20dp"
                android:adjustViewBounds="true"
                android:padding="10dp"
                android:src="@drawable/icon_bills" />


        </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>

